  "scripts": {
    "build": "npm run clean && gulp build",
    "check-licenses": "gulp check-licenses",
    "check-package-lock": "git diff-index --quiet HEAD -- package-lock.json || (echo 'package-lock.json is dirty!' && exit 1)",
    "clean": "rm -rf build && rm -rf dist && git clean -X -f",
    "dev": "npm run clean && gulp dist && npm link && gulp watch-dist",
    "dist": "npm run clean && ./bin/check-if-clean && gulp dist",
    "fix-format": "node_modules/.bin/tslint -p . --fix && node_modules/.bin/tsfmt -r",
    "generate-docs": "gulp generate-docs",
    "integ": "./test/integ/run",
    "lint": "gulp lint",
    "prepare": "npm run snyk-protect",
    "prepublishOnly": "npm run dist",
    "preversion": "npm test",
    "snyk-protect": "snyk protect",
    "test": "npm run clean && gulp test",
    "update-submodules": "git submodule update --init --recursive --jobs 4",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "view-report": "open coverage/index.html",
    "watch": "gulp watch"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E GIT_PARAMS",
      "pre-push": "npm run check-package-lock && npm test && npm run integ"
    }
  },
  "snyk": true
}
